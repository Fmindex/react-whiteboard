{
  "name": "react-whiteboard",
  "description": "A whiteboard React component using SVG",
  "version": "0.0.2",
  "author": "Kenichi Ohtomi",
  "license": "MIT",
  "homepage": "https://github.com/ohtomi/react-whiteboard",
  "private": true,
  "scripts": {
    "build": "run-s lint compile test",
    "clear": "rimraf dist coverage .nyc_output",
    "lint": "eslint src/ test/",
    "compile": "cross-env NODE_ENV=production webpack --progress --profile",
    "test": "cross-env NODE_ENV=test ava --verbose",
    "cover": "nyc --reporter=html --reporter=text yarn test",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "watch": "chokidar test/**/*.js -c \"cross-env NODE_ENV=test ava --tap {path} | tap-notify\""
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-proptypes": "^1.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-ava": "^4.0.0",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-security": "^1.2.0",
    "flow-bin": "^0.46.0",
    "flow-typed": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-proptypes": "^1.0.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sinon": "^2.2.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}"
    ],
    "require": [
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohtomi/react-whiteboard.git"
  },
  "bugs": {
    "url": "https://github.com/ohtomi/react-whiteboard/issues"
  }
}
